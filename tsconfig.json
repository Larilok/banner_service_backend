{
  "compilerOptions": {
      "module": "es6",
      "esModuleInterop": true,
      "target": "es3",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "outDir": "dist",
      "baseUrl": ".",
      "experimentalDecorators": true,
      "paths": {
          "*": [
              "node_modules/*",
              "src/types/*"
          ]
      }
  },
  "include": [
    //   "src/**/*"
    "src/modules/Typescript"
  ]
}